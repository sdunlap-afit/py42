{
	"name": "llvm development container",
	"dockerFile": "./Dockerfile",
	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker:2": {}
	},
	"workspaceMount": "source=${localWorkspaceFolder},target=/home/user/py42,type=bind",
	"workspaceFolder": "/home/user/py42",
	"remoteUser": "user",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-vscode.makefile-tools",
				"ms-vscode.cpptools-extension-pack",
				"ms-toolsai.jupyter"
			]
		}
	}
}
